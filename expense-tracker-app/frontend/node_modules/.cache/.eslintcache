os.post(`${BASE_URL_INCOME}add-income`, income)\n            .catch((err) =>{\n                setError(err.response.data.message)\n            })\n        getIncomes()\n    }\n\n    const getIncomes = async () => {\n        const response = await axios.get(`${BASE_URL_INCOME}get-income`)\n        setIncomes(response.data)\n        console.log(response.data)\n    }\n\n    const deleteIncome = async (id) => {\n        const res  = await axios.delete(`${BASE_URL_INCOME}delete-income/${id}`)\n        getIncomes()\n    }\n\n    const totalIncome = () => {\n        let totalIncome = 0;\n        incomes.forEach((income) =>{\n            totalIncome = totalIncome + income.amount\n        })\n\n        return totalIncome;\n    }\n\n\n    //calculate incomes\n    const addExpense = async (income) => {\n        const response = await axios.post(`${BASE_URL_EXPENSE}add-expense`, income)\n            .catch((err) =>{\n                setError(err.response.data.message)\n            })\n        getExpenses()\n    }\n\n    const getExpenses = async () => {\n        const response = await axios.get(`${BASE_URL_EXPENSE}get-expense`)\n        setExpenses(response.data)\n        console.log(response.data)\n    }\n\n    const deleteExpense = async (id) => {\n        const res  = await axios.delete(`${BASE_URL_EXPENSE}delete-expense/${id}`)\n        getExpenses()\n    }\n\n    const totalExpenses = () => {\n        let totalIncome = 0;\n        expenses.forEach((income) =>{\n            totalIncome = totalIncome + income.amount\n        })\n\n        return totalIncome;\n    }\n\n\n    const totalBalance = () => {\n        return totalIncome() - totalExpenses()\n    }\n\n    const transactionHistory = () => {\n        const history = [...incomes, ...expenses]\n        history.sort((a, b) => {\n            return new Date(b.createdAt) - new Date(a.createdAt)\n        })\n\n        return history.slice(0, 3)\n    }\n\n\n    return (\n        <GlobalContext.Provider value={{\n            addIncome,\n            getIncomes,\n            incomes,\n            deleteIncome,\n            expenses,\n            totalIncome,\n            addExpense,\n            getExpenses,\n            deleteExpense,\n            totalExpenses,\n            totalBalance,\n            transactionHistory,\n            error,\n            setError\n        }}>\n            {children}\n        </GlobalContext.Provider>\n    )\n}\n\nexport const useGlobalContext = () =>{\n    return useContext(GlobalContext)\n}\n",[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/styles/Layouts.js",[],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/Components/Expenses/Expenses.js",["108"],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/Components/Orb/Orb.js",[],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/Components/Income/Income.js",["109","110"],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/Components/Navigation/Navigation.js",["111"],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/Components/Dashboard/Dashboard.js",["112"],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/Components/Expenses/ExpenseForm.js",[],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/Components/IncomeItem/IncomeItem.js",[],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/Components/Form/Form.js",["113"],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/utils/menuItems.js",[],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/utils/Icons.js",[],[],"/home/tabeedhassan/go/src/Fun-Application-Projects/expense-tracker-app/frontend/src/utils/useWindowSize.js",[],[],"/home/tabeedhassan/go/src/Fu